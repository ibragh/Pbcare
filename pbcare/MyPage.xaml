<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" 
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
x:Class="pbcare.MyPage"
xmlns:myapp="clr-namespace:pbcare;assembly=pbcare">
	
<TabbedPage.Children>
 		<NavigationPage  Title="حملي" >
        <x:Arguments>
       		<myapp:PregnancyPage/>
	   	</x:Arguments>
        </NavigationPage>
 
        <NavigationPage Title="أطفالي">
        		<x:Arguments>
       				<myapp:BabyPage/>
	   			</x:Arguments>
        </NavigationPage>
 
        <ContentPage Title="المنتدى" />
  
        <NavigationPage  Title="الظبط" >
		   <x:Arguments>
       		<myapp:Setting/>
	   	</x:Arguments>
		</NavigationPage >
</TabbedPage.Children>
	
</TabbedPage>
